<Window x:Class="VisionRobot.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Visión" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10" />
            <RowDefinition Height="277*" />
            <RowDefinition Height="27" />
            <RowDefinition Height="10" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10" />
            <ColumnDefinition Width="97" />
            <ColumnDefinition Width="386*" />
            <ColumnDefinition Width="10" />
        </Grid.ColumnDefinitions>
        <Image Name="image1" Stretch="Fill"  Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="2" />
        <Button Content="Iniciar" Grid.Column="1" Grid.Row="2"   Name="button1" Click="button1_Click" />
    </Grid>
</Window>
